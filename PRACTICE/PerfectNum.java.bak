import java.util.*;
class PerfectNum
{
	public static void main(String[] args) 
	{
		Scanner s=new Scanner(System.in);
		System.out.println("enter number ");
		int num=s.nextInt();
		int sum=0;
		for (inti=1;i<num ;i++ )
		{
			if (num%i==0)
			{
				sum=sum+i;
			}
		}
		if (sum==num)
		{
			System.out.println(num +" is a perfect number");
		}
		else{
			
			System.out.println(num + " is not a perfect number");
		}
	}
}
