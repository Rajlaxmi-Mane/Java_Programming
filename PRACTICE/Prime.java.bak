import java.util.*;
class Prime 
{
	public static void main(String[] args) 
	{
		Scanner s=new Scanner(System.in);
		int t=0;
		System.out.println("enter num");
		int num=s.nextInt();
		for (int i=2;i<num ;i++ )
		{
			if(num%i==0)
			{
				t=1;
				return;
			}
		}
		if (t==1)
		{
			System.out.println(num+" is not a prime number");
		}
		else{
			System.out.println(num+" is a prime number");
		}
	}
}
